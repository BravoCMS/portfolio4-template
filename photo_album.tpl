<{extends 'layout/layout.tpl'}><{block name="template-name"}>    /photo_album.tpl<{/block}><{block name=title}>    <{$photo_album.title|htmlspecialchars}><{/block}><{block name="module"}>    <h1 class="selected_filters_left">        <{$photo_album.name|htmlspecialchars}>    </h1>    <div class="product_details_photo" id="gallery">        <div id="wrapper">            <div id="content">                <div class="cover-93 fullscreen-cover" data-bg-type="image" data-cover-id="93">                    <div class="cover-image" data-image-zoom="zoom"></div>                    <div class="see-more">                        <div class="icon">                            <svg version="1.1" id="Ebene_2_1_" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="53px" height="20px" viewBox="0 0 53 20" enable-background="new 0 0 53 20" xml:space="preserve">                                <g><polygon points="26.5,17.688 9.114,3.779 10.303,2.312 26.5,15.269 42.697,2.313 43.886,3.779 	"></polygon></g>                            </svg>                        </div>                    </div>                </div>                <div class="block_conteiner_wrp">                    <h3>                        <{$photo_album.name}>                    </h3>                    <div class="album-description out-ext-info">                        <{$photo_album.text}>                    </div>                    <div class="block_conteiner_wrp_inner">                        <div class="block_conteiner_gallery photoswipe-slider">                            <{foreach $photo_album.photos as $photo}>                                <figure class="block_conteiner">                                    <a href="<{$photo.photo_file}>" data-size="<{$photo.photo_width}>x<{$photo.photo_height}>">                                        <img src="<{$photo.photo_file_small}>" alt="<{$photo.photo_title}>" class="img-responsive gallery-pic" />                                    </a>                                    <figcaption><{$photo.photo_description}></figcaption>                                </figure>                            <{/foreach}>                        </div>                    </div>                </div>            </div>        </div>    </div>    <div class="album_description_additional out-ext-info">        <{$photo_album.extra}>    </div><{/block}>